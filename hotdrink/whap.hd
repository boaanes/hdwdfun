import { sqrt } from './bo.js'

component whap {

  var w: number = 10, h: number = 10, a: number, p: number;

  constraint awh {
    area(w, h -> a) => w*h;
    widthAndHeight(a -> w, h) => [sqrt(a), sqrt(a)];
  }

  constraint pwh {
    perimiter(w, h -> p) => 2*(w+h);
    width(p, h -> w) => p/2 - h;
    height(p, w -> h) => p/2 - w;
  }

}